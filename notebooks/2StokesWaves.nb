Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Kinematic", " ", "BC", " ", "at", " ", "all", " ", "orders"}], 
    " ", "-", " ", 
    RowBox[{
    "Particles", " ", "on", " ", "Surface", " ", "Stay", " ", "on", " ", 
