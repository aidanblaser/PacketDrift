Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Ex", ".", " ", "KBC"}], " ", "at", " ", "order", " ", 
    "\[Epsilon]"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"TrigReduce", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"kbc", "[", 
       RowBox[{"[", "2", "]"}], "]"}], " ", "==", " ", "0"}], "  ", "/.", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"k1", " ", "x"}], " ", "-", " ", 
         RowBox[{"omega1", " ", "t"}]}], " ", "->", " ", "\[Theta]1"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"k2", " ", "x"}], " ", "-", " ", 
