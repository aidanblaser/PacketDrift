475c664b0cce"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "\"\<Global`*`\>\"", "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Define", " ", "variables"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"order", " ", "=", " ", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"omega1", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Omega]1", "[", "n", "]"}], 
      RowBox[{"\[Epsilon]", "^", "n"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "0", ",", 
       RowBox[{"order", "-", "1"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"omega2", " ", "=", " ", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Omega]2", "[", "n", "]"}], 
       RowBox[{"\[Epsilon]", "^", "n"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "0", ",", 
        RowBox[{"order", "-", "1"}]}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Expand", " ", "in", " ", "Fourier", " ", "Series"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"phi1", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Epsilon]", "^", "n"}], " ", "*", " ", 
      RowBox[{"\[Phi]1", "[", 
       RowBox[{"n", ",", "m"}], "]"}], 
      RowBox[{"Sin", "[", 
       RowBox[{"m", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k1", " ", "x"}], " ", "-", " ", 
          RowBox[{"omega1", " ", "t"}]}], ")"}]}], "]"}], 
      RowBox[{"Exp", "[", 
       RowBox[{"m", " ", "k1", " ", "z"}], "]"}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"n", ",", "1", ",", "order"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "1", ",", "order"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"phi2", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Epsilon]", "^", "n"}], " ", "*", " ", 
      RowBox[{"\[Phi]2", "[", 
       RowBox[{"n", ",", "m"}], "]"}], 
      RowBox[{"Sin", "[", 
       RowBox[{"m", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k2", " ", "x"}], " ", "-", " ", 
          RowBox[{"omega2", " ", "t"}]}], ")"}]}], "]"}], 
      RowBox[{"Exp", "[", 
       RowBox[{"m", " ", "k2", " ", "z"}], "]"}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"n", ",", "1", ",", "order"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "1", ",", "order"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"phi", " ", "=", " ", 
    RowBox[{"phi1", " ", "+", " ", "phi2"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eta1", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Epsilon]", "^", "n"}], " ", "*", " ", 
      RowBox[{"\[Eta]1", "[", 
       RowBox[{"n", ",", "m"}], "]"}], "*", 
      RowBox[{"Cos", "[", 
       RowBox[{"m", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k1", " ", "x"}], " ", "-", " ", 
          RowBox[{"omega1", " ", "t"}]}], ")"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "1", ",", "order"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "1", ",", "order"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eta2", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Epsilon]", "^", "n"}], " ", "*", " ", 
      RowBox[{"\[Eta]2", "[", 
       RowBox[{"n", ",", "m"}], "]"}], "*", 
      RowBox[{"Cos", "[", 
       RowBox[{"m", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k2", " ", "x"}], " ", "-", " ", 
          RowBox[{"omega2", " ", "t"}]}], ")"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "1", ",", "order"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "1", ",", "order"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eta", " ", "=", " ", 
   RowBox[{"eta1", " ", "+", " ", "eta2"}]}], 
  ";"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.8753725536054697`*^9, 3.875372782267696*^9}, {
  3.8753729427613297`*^9, 3.87537295739353*^9}, {3.87537301234898*^9, 
  3.8753730742680817`*^9}, {3.87537313840974*^9, 3.875373147335505*^9}, {
  3.8753732572280684`*^9, 3.875373294355809*^9}, {3.8753738889994*^9, 
  3.8753738961924877`*^9}, {3.875374043435609*^9, 3.875374049749517*^9}, {
  3.875374088985095*^9, 3.875374090624946*^9}},
 CellLabel->
